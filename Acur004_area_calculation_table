/***** End of imports. If edited, may not auto-convert in the playground. *****/
var num = 1;

var image = ee.ImageCollection ('projects/mapbiomas-workspace/AMOSTRAS/GTAGUA/ACURACIA/V11/controlImage')
  .filter(ee.Filter.eq("CD_Bioma", num)).first();
   
var grids = ee.FeatureCollection("users/frederickirchhoff/brazil_big_grids");
var bioms = ee.FeatureCollection('projects/mapbiomas-workspace/AUXILIAR/biomas-2019');

var centroids = grids.map(function (grid) { 
  return grid.centroid();
});

Map.addLayer (image) 
Map.addLayer (grids)
Map.addLayer (centroids)

var list_biom = ee.List.sequence(num, num, 1); 

var featureList = list_biom.map(function (biom) {
  
  var img = ee.ImageCollection ('projects/mapbiomas-workspace/AMOSTRAS/GTAGUA/ACURACIA/V11/controlImage')
  .filter(ee.Filter.eq("CD_Bioma", biom)).first();
  
  var centroidSamples = img.sampleRegions({collection: centroids, scale: 500, geometries: false});
  var class_1 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["permanente"]).get("sum");
  var class_2 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["intermitente"]).get("sum");
  var class_3 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["infrequente"]).get("sum");
  var class_4 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["solo"]).get("sum");
  var class_5 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["dist_250"]).get("sum");
  var class_6 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["dist_500"]).get("sum");
  var class_7 = centroidSamples.reduceColumns(ee.Reducer.sum(), ["dist_5000"]).get("sum");
  
  return ee.Feature(null, {
    'biom': biom,
    "permanente": class_1,
    "intermitente": class_2,
    "infrequente": class_3,
    "solo": class_4,
    "dist_250": class_5,
    "dist_500": class_6,
    "dist_5000": class_7
  });
});

var collection = ee.FeatureCollection(featureList);

print(collection)

Export.table.toDrive(collection, "control_areas_" + num, "areas", "control_areas_" + num, "csv");

